<p id="notice"><%= notice %></p>

<h1>Listing Liquidacions</h1>

<table>
  <thead>
    <tr>
      <th>Fecha emision</th>
      <th>Asignacion</th>
      <th>Habere</th>
      <th>Descuento adicional</th>
      <th>Bonificacion</th>
      <th>Descuento</th>
      <th>Personal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @liquidacions.each do |liquidacion| %>
      <tr>
        <td><%= liquidacion.fecha_emision %></td>
        <td><%= liquidacion.asignacion %></td>
        <td><%= liquidacion.habere %></td>
        <td><%= liquidacion.descuento_adicional %></td>
        <td><%= liquidacion.bonificacion %></td>
        <td><%= liquidacion.descuento %></td>
        <td><%= liquidacion.personal %></td>
        <td><%= link_to 'Show', liquidacion %></td>
        <td><%= link_to 'Edit', edit_liquidacion_path(liquidacion) %></td>
        <td><%= link_to 'Destroy', liquidacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Liquidacion', new_liquidacion_path %>
